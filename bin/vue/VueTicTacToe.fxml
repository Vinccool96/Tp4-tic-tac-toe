<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckMenuItem?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="root" styleClass="border-pane"
	stylesheets="@../../res/styles/tictactoe.css" xmlns="http://javafx.com/javafx/8.0.60"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.ControleurTicTacToe">
	<center>
		<GridPane fx:id="gridPane" hgap="20.0" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="600.0" prefWidth="600.0" vgap="20.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Button fx:id="btnHG" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedHG" prefHeight="200.0" prefWidth="200.0"
					GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
				<Button fx:id="btnHC" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedHC" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.halignment="CENTER"
					GridPane.valignment="CENTER" />
				<Button fx:id="btnHD" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedHD" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="2" GridPane.halignment="CENTER"
					GridPane.valignment="CENTER" />
				<Button fx:id="btnCG" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedCG" prefHeight="200.0" prefWidth="200.0"
					GridPane.halignment="CENTER" GridPane.rowIndex="1"
					GridPane.valignment="CENTER" />
				<Button fx:id="btnCC" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedCC" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.halignment="CENTER"
					GridPane.rowIndex="1" GridPane.valignment="CENTER" />
				<Button fx:id="btnCD" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedCD" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="2" GridPane.halignment="CENTER"
					GridPane.rowIndex="1" GridPane.valignment="CENTER" />
				<Button fx:id="btnBG" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedBG" prefHeight="200.0" prefWidth="200.0"
					GridPane.halignment="CENTER" GridPane.rowIndex="2"
					GridPane.valignment="CENTER" />
				<Button fx:id="btnBC" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedBC" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.halignment="CENTER"
					GridPane.rowIndex="2" GridPane.valignment="CENTER" />
				<Button fx:id="btnBD" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
					onAction="#clickedBD" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="2" GridPane.halignment="CENTER"
					GridPane.rowIndex="2" GridPane.valignment="CENTER" />
			</children>
			<BorderPane.margin>
				<Insets bottom="50.0" left="50.0" right="50.0" />
			</BorderPane.margin>
		</GridPane>
	</center>
	<top>
		<VBox BorderPane.alignment="CENTER">
			<children>
				<MenuBar fx:id="menuBar">
					<menus>
						<Menu mnemonicParsing="false" text="Fichier">
							<items>
								<MenuItem fx:id="recommencerItem" mnemonicParsing="false"
									text="Recommencer" />
								<MenuItem fx:id="quitterItem" mnemonicParsing="false"
									text="Quitter" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Niveau">
							<items>
								<CheckMenuItem fx:id="aleatoireCheckItem"
									mnemonicParsing="false" selected="true" text="AlÃ©atoire" />
								<CheckMenuItem fx:id="facileCheckItem"
									mnemonicParsing="false" text="Facile" />
								<CheckMenuItem fx:id="aiCheckItem"
									mnemonicParsing="false" text="AI" />
							</items>
						</Menu>
					</menus>
				</MenuBar>
				<GridPane alignment="CENTER">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
							prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
							prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label fx:id="lblDifficulte" styleClass="label-difficulty"
							text="Label" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
						<Label fx:id="lblChronometre" styleClass="label-time" text="Label"
							GridPane.columnIndex="1" GridPane.halignment="CENTER"
							GridPane.valignment="CENTER" />
					</children>
					<padding>
						<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
					</padding>
				</GridPane>
			</children>
		</VBox>
	</top>
	<bottom>
		<GridPane BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Button fx:id="recommencerButton" mnemonicParsing="false"
					styleClass="button-bottom" text="Recommencer" GridPane.halignment="CENTER"
					GridPane.valignment="CENTER">
					<GridPane.margin>
						<Insets />
					</GridPane.margin>
				</Button>
				<Button fx:id="quitterButton" mnemonicParsing="false"
					styleClass="button-bottom" text="Quitter" GridPane.columnIndex="1"
					GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
			</children>
			<padding>
				<Insets bottom="50.0" left="10.0" right="10.0" />
			</padding>
		</GridPane>
	</bottom>
</BorderPane>
